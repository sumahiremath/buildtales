  "severity_levels": {
    "critical": "Immediate action required, may require re-enrollment",
    "high": "Update required before next ACH file",
    "medium": "Update recommended, affects compliance",
    "low": "Minor issue, update when convenient"
  },
  "common_occurrence_levels": {
    "very_common": "Seen frequently in production environments",
    "common": "Regular occurrence, should be handled automatically",
    "uncommon": "Occasional occurrence, manual review may be needed",
    "rare": "Seldom seen, may indicate unusual circumstances"
  },
  "implementation_notes": {
    "parsing": "Look for SEC Code = COR in Entry Detail records",
    "extraction": "NOC code is typically in positions 79-80 of Entry Detail record",
    "logging": "Always log corrections with timestamp and operator information",
    "compliance": "Corrections must be applied within 6 banking days or before next entry"
  }
